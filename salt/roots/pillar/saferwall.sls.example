            requests:
              memory: 2100Mi
            limits:
              memory: 2200Mi
        comodo:
          enabled: true
          port:
            container: 10005
            host: 10005
          resources:
            requests:
              memory: 300Mi
            limits:
              memory: 350Mi
        sophos:
          enabled: true
          port:
            container: 10011
            host: 10011
          resources:
            requests:
              memory: 300Mi
            limits:
              memory: 350Mi
        windefender:
          enabled: true
